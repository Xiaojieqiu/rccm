## The function is currently defined as
function (x, dists, index, nn) 
{
    E <- ncol(x)
    u <- exp(-dists[index, nn[index, ]]/dists[index, nn[index, 
        1]])
    w <- u/sum(u)
    return(list(w = w, nn = nn[index, ]))
  }
}
